  object adoFamily: TADOQuery
    Connection = dbMain.adoSocialWorkDB
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select *'
      
        '       ,(select RelationshipName from Relationships where Relati' +
        'onships.RelationshipID = families.RelationshipID) as Relationshi' +
        'pName'
      '  from families')
    Left = 152
    Top = 102
  end
